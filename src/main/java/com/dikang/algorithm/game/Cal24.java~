public class Cal24 {
	
	private char[] op = new char[] {'+', '-', '*', '/'};
	
	private double calculate(double a, double b, char opNo) {
		switch(opNo) {
			case '+':
				return a + b;
			case '-':
				return a - b;
			case '*':
				return a * b;
			case '/':
				return a / b;
		}
		
		return 0;
	}
	
	private int[] getNextIndex(int[] cur) {
		
	}
	
	public static void main(String[] args) {
		
	}

}
